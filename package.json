{
  "name": "@hiogawa/toy-metronome",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist src/audioworklet/build src/service-worker/build .vercel/output",
    "dev": "pnpm build:tsup && pnpm run --stream /^dev:/",
    "dev:tsup": "tsup --watch",
    "dev:vite": "vite --no-clearScreen",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "build": "pnpm run --seq /^build:/",
    "build:tsup": "tsup",
    "build:vite": "vite build",
    "build:vercel": "bash misc/vercel/build.sh",
    "preview": "vite preview",
    "generate-tw": "unocss-typescript-dsl --outDir src/styles/tw",
    "tsc": "tsc",
    "lint": "pnpm run --seq /^lint:/",
    "lint-check": "pnpm run --seq /^lint-check:/",
    "lint:isort": "isort-ts --git --cache --fix",
    "lint-check:isort": "isort-ts --git --cache",
    "lint:prettier": "prettier --write --cache .",
    "lint-check:prettier": "prettier --check --cache .",
    "release": "vercel deploy --prebuilt .",
    "release-production": "vercel deploy --prebuilt . --prod"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@hiogawa/theme-script": "0.0.4-pre.3",
    "@hiogawa/tiny-rpc": "0.2.3-pre.11",
    "@hiogawa/utils": "1.6.1-pre.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@hiogawa/isort-ts": "1.1.2-pre.0",
    "@hiogawa/unocss-preset-antd": "^2.0.1",
    "@hiogawa/unocss-typescript-dsl": "2.0.0-pre.6",
    "@iconify-json/ri": "^1.1.4",
    "@tsconfig/strictest": "^2.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@unocss/reset": "^0.47.5",
    "@vitejs/plugin-react": "^2.2.0",
    "prettier": "^2.8.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unocss": "^0.47.5",
    "vite": "^3.2.4",
    "workbox-build": "^6.5.4",
    "workbox-cli": "^6.5.4"
  },
  "volta": {
    "node": "18.16.0"
  }
}
