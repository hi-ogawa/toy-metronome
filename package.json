{
  "scripts": {
    "dev": "pnpm dev-setup && run-p dev:*",
    "dev:vite": "vite --no-clearScreen",
    "dev:audioworklet": "npm run build:audioworklet -- --watch",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev-setup": "run-s build:audioworklet",
    "build": "run-s build:*",
    "build:audioworklet": "esbuild ./src/audioworklet/index.ts --bundle --outdir=./src/audioworklet/build",
    "lint": "run-s lint:*",
    "lint-check": "run-s lint-check:*",
    "lint:prettier": "prettier --write --cache .",
    "lint-check:prettier": "prettier --check --cache .",
    "lint:eslint": "npm run lint-check:eslint -- --fix",
    "lint-check:eslint": "eslint . --cache --cache-location node_modules/.cache/.eslintcache --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "eslint": "^8.29.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-use": "^17.4.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@unocss/reset": "^0.47.5",
    "@vitejs/plugin-react": "^2.2.0",
    "esbuild": "^0.15.16",
    "eslint-plugin-import": "^2.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3",
    "unocss": "^0.47.5",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  }
}
